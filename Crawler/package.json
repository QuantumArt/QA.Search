{
  "name": "crawler",
  "version": "1.0.0",
  "description": "Crawler for MTS sites",
  "main": "src/index.js",
  "scripts": {
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "http://tfs01:8080/tfs/QuantumartCollection/_git/MTS.Search"
  },
  "author": "Quantum Art",
  "license": "UNLICENSED",
  "dependencies": {
    "path-to-regexp": "^3.0.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "coveralls": "^3.0.2",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,ts,jsx,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".+\\.test\\.(jsx?|tsx?)$"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "transform": {
      "^.+\\.(jsx?|tsx?)$": "ts-jest"
    },
    "testRegex": ".+\\.test\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "jsx",
      "tsx"
    ]
  }
}
